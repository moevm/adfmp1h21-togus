<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/menuFrag">
    <fragment
        android:id="@+id/menuFrag"
        android:name="com.example.toguz.MenuFrag"
        android:label="menu_lay"
        tools:layout="@layout/menu_lay" >
        <action
            android:id="@+id/action_menuFrag_to_pvcFrag"
            app:destination="@id/pvcFrag" />
        <action
            android:id="@+id/action_menuFrag_to_statFrag"
            app:destination="@id/statFrag" />
        <action
            android:id="@+id/action_menuFrag_to_instructFrag"
            app:destination="@id/instructFrag" />
        <action
            android:id="@+id/action_menuFrag_to_pvpFrag"
            app:destination="@id/pvpFrag" />
    </fragment>
    <fragment
        android:id="@+id/instructFrag"
        android:name="com.example.toguz.instructFrag"
