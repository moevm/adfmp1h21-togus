            android:id="@+id/action_pvcFrag_to_menuFrag"
            app:destination="@id/menuFrag" />
    </fragment>
    <fragment
        android:id="@+id/pvpFrag"
        android:name="com.example.toguz.PvpFrag"
        android:label="pvp_lay"
        tools:layout="@layout/pvp_lay" >
        <action
            android:id="@+id/action_pvpFrag_to_menuFrag"
            app:destination="@id/menuFrag" />
    </fragment>
    <fragment
        android:id="@+id/statFrag"
        android:name="com.example.toguz.statFrag"
        android:label="stat_lay"
        tools:layout="@layout/stat_lay" >
        <action
            android:id="@+id/action_statFrag_to_menuFrag"
            app:destination="@id/menuFrag" />
    </fragment>
</navigation>